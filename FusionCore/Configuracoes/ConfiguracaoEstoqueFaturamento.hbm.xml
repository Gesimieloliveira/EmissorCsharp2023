<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping
  assembly="FusionCore"
  namespace="FusionCore.Configuracoes"
  default-lazy="false"
  xmlns="urn:nhibernate-mapping-2.2">

  <class name="ConfiguracaoEstoqueFaturamento" table="configuracao_estoque_faturamento">
    <id name="Id">
      <column name="id" not-null="true" />
    </id>

    <property name="MovimentarEstoque">
      <column name="movimentarEstoque" not-null="true" />
    </property>
  </class>
</hibernate-mapping>