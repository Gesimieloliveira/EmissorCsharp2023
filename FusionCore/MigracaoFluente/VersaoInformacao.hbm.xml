<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping assembly="FusionCore" 
                   namespace="FusionCore.MigracaoFluente" 
                   default-lazy="false"
                   xmlns="urn:nhibernate-mapping-2.2">

  <class name="VersaoInformacao" table="versao_informacao">
    <id name="VersaoAplicada" column="versao" access="backfield" />
    <property access="backfield" name="AplicadoEm" column="aplicadoEm" not-null="true" />
  </class>
</hibernate-mapping>