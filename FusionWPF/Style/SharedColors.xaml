<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:wl="clr-namespace:Fusion.WhiteLabel;assembly=Fusion.WhiteLabel">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="{x:Static wl:MarcaWhiteLabel.CorSoftware}" />
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{DynamicResource MahApps.Colors.Accent}" />
    <SolidColorBrush x:Key="SecondaryBrush" Color="{DynamicResource MahApps.Colors.Accent2}" />
    <SolidColorBrush x:Key="MenuIconBrush" Color="{DynamicResource MahApps.Colors.Accent2}" />

    <SolidColorBrush x:Key="WhiteBrush" Color="#ffffff" />
    <SolidColorBrush x:Key="BlackBrush" Color="#000000" />
    <SolidColorBrush x:Key="SuccessBrush" Color="#449d44" />
    <SolidColorBrush x:Key="InfoBrush" Color="#31b0d5" />
    <SolidColorBrush x:Key="DangerBrush" Color="#d9534f" />
    <SolidColorBrush x:Key="DangerBrush2" Color="#ff0000" />
    <SolidColorBrush x:Key="WarningBrush" Color="#ec971f" />
    <SolidColorBrush x:Key="RedOnlyForegroundBrush" Color="#505050" />
    <SolidColorBrush x:Key="ReadOnlyBackgroundBrush" Color="#e8e8e8" />
    <SolidColorBrush x:Key="IsEnableOffButtonBrush" Color="#A9A9A9" />
    <SolidColorBrush x:Key="IsEnableOffButtonForeground" Color="#E1E1E1" />

    <SolidColorBrush x:Key="MahApps.Brushes.DataGrid.Selection.Background" Color="{DynamicResource MahApps.Colors.Accent2}" />
    <SolidColorBrush x:Key="MahApps.Brushes.DataGrid.Selection.Background.Disabled" Color="{DynamicResource MahApps.Colors.Gray7}" />
    <SolidColorBrush x:Key="MahApps.Brushes.DataGrid.Selection.Text" Color="{DynamicResource MahApps.Colors.IdealForeground}" />
    <SolidColorBrush x:Key="MahApps.Brushes.DataGrid.Selection.Background.MouseOver" Color="{DynamicResource MahApps.Colors.Accent3}" />
    <SolidColorBrush x:Key="MahApps.Brushes.DataGrid.Selection.BorderBrush.Focus" Color="{DynamicResource MahApps.Colors.Accent}" />
    <SolidColorBrush x:Key="MahApps.Brushes.DataGrid.Selection.Background.Inactive" Color="{DynamicResource MahApps.Colors.Accent2}"/>
    <SolidColorBrush x:Key="MahApps.Brushes.DataGrid.Selection.Text.Inactive" Color="{DynamicResource MahApps.Colors.IdealForeground}" />

    <!-- compatibility mahapp -->
    <SolidColorBrush x:Key="AccentColorBrush" Color="{DynamicResource MahApps.Colors.Accent}" />
    <SolidColorBrush x:Key="AccentColorBrush2" Color="{DynamicResource MahApps.Colors.Accent2}" />
    <SolidColorBrush x:Key="AccentColorBrush3" Color="{DynamicResource MahApps.Colors.Accent3}" />
    <SolidColorBrush x:Key="AccentColorBrush4" Color="{DynamicResource MahApps.Colors.Accent4}" />
    <SolidColorBrush x:Key="IdealForegroundColorBrush" Color="{DynamicResource MahApps.Colors.IdealForeground}" />

    <SolidColorBrush x:Key="GrayBrush" Color="{DynamicResource MahApps.Colors.Gray}" />
    <SolidColorBrush x:Key="GrayBrush1" Color="{DynamicResource MahApps.Colors.Gray1}" />
    <SolidColorBrush x:Key="GrayBrush2" Color="{DynamicResource MahApps.Colors.Gray2}" />
    <SolidColorBrush x:Key="GrayBrush3" Color="{DynamicResource MahApps.Colors.Gray3}" />
    <SolidColorBrush x:Key="GrayBrush4" Color="{DynamicResource MahApps.Colors.Gray4}" />
    <SolidColorBrush x:Key="GrayBrush5" Color="{DynamicResource MahApps.Colors.Gray5}" />
    <SolidColorBrush x:Key="GrayBrush6" Color="{DynamicResource MahApps.Colors.Gray6}" />
    <SolidColorBrush x:Key="GrayBrush7" Color="{DynamicResource MahApps.Colors.Gray7}" />
    <SolidColorBrush x:Key="GrayBrush8" Color="{DynamicResource MahApps.Colors.Gray8}" />
    <SolidColorBrush x:Key="GrayBrush9" Color="{DynamicResource MahApps.Colors.Gray9}" />
    <SolidColorBrush x:Key="GrayBrush10" Color="{DynamicResource MahApps.Colors.Gray10}" />

</ResourceDictionary>